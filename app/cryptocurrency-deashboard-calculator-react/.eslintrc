{
  "env": {
      "browser": true,
      "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "ecmaFeatures": {
      "modules": true,
      "jsx": true,
      "spread" : true,
      "restParams" : true
  },
  "rules" : {
      "no-unused-vars" : 0,
      "no-undef" : 0,
      "no-console": 0,
      "no-alert": 0
  }
}
